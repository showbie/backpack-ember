  "scripts": {
    "build": "ember build",
    "build:storybook": "ember build && build-storybook --config-dir .storybook -s dist --output-dir docs/storybook",
    "format:js": "prettier-eslint '**/*.js' --write",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "prettier-eslint '**/*.js' --list-different",
    "start": "ember serve",
    "storybook": "ember serve & start-storybook -p 9001 -s dist",
    "test": "ember test",
    "test:all": "ember try:each"
  },
