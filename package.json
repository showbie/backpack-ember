{
  "name": "@showbie/backpack-ember",
  "version": "0.0.0",
  "description": "Ember components for Showbieâ€™s Backpack design system.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "showbie/backpack-ember",
  "license": "MIT",
  "author": "Showbie (https://www.showbie.com)",
  "maintainers": [
    "Jeff Nelson @stormwarning (https://www.showbie.com)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changeset": "changeset add",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "release": "npm run build && changeset publish",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css}": [
      "stylelint --fix"
    ],
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.hbs": [
      "ember-template-lint --fix"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.hbs",
        "options": {
          "htmlWhitespaceSensitivity": "css",
          "parser": "glimmer",
          "singleQuote": false
        }
      }
    ]
  },
  "dependencies": {
    "ember-cli-babel": "7.23.1",
    "ember-cli-htmlbars": "5.6.2",
    "ember-cli-typescript": "4.1.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.2.8",
    "@changesets/cli": "2.14.x",
    "@ember/optional-features": "2.0.x",
    "@ember/test-helpers": "2.2.x",
    "@glimmer/component": "1.0.x",
    "@glimmer/tracking": "1.0.x",
    "@showbie/backpack-tokens": "2.8.x",
    "@showbie/eslint-config": "3.0.x",
    "@tailwindcss/typography": "0.4.0",
    "@types/ember-qunit": "3.4.x",
    "@types/ember-resolver": "5.0.x",
    "@types/ember__application": "3.16.x",
    "@types/ember__array": "3.16.x",
    "@types/ember__component": "3.16.x",
    "@types/ember__controller": "3.16.x",
    "@types/ember__debug": "3.16.x",
    "@types/ember__engine": "3.16.x",
    "@types/ember__error": "3.16.x",
    "@types/ember__object": "3.12.x",
    "@types/ember__polyfills": "3.12.x",
    "@types/ember__routing": "3.16.x",
    "@types/ember__runloop": "3.16.x",
    "@types/ember__service": "3.16.x",
    "@types/ember__string": "3.16.x",
    "@types/ember__template": "3.16.x",
    "@types/ember__test": "3.16.x",
    "@types/ember__test-helpers": "1.7.x",
    "@types/ember__utils": "3.16.x",
    "@types/htmlbars-inline-precompile": "1.0.x",
    "@types/qunit": "2.11.x",
    "@types/rsvp": "4.0.x",
    "@typescript-eslint/eslint-plugin": "4.15.x",
    "@typescript-eslint/parser": "4.15.x",
    "autoprefixer": "10.2.x",
    "babel-eslint": "10.1.x",
    "broccoli-asset-rev": "3.0.x",
    "ember-auto-import": "1.10.x",
    "ember-cli": "3.25.0",
    "ember-cli-dependency-checker": "3.2.x",
    "ember-cli-deploy": "1.0.x",
    "ember-cli-deploy-build": "2.0.x",
    "ember-cli-deploy-git": "1.3.x",
    "ember-cli-fastboot": "2.2.x",
    "ember-cli-inject-live-reload": "2.0.x",
    "ember-cli-postcss": "7.0.x",
    "ember-cli-sri": "2.1.x",
    "ember-cli-terser": "4.0.x",
    "ember-cli-typescript-blueprints": "3.0.x",
    "ember-disable-prototype-extensions": "1.1.x",
    "ember-export-application-global": "2.0.x",
    "ember-load-initializers": "2.1.x",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-page-title": "6.2.x",
    "ember-qunit": "5.1.x",
    "ember-resolver": "8.0.x",
    "ember-source": "3.25.1",
    "ember-source-channel-url": "3.0.x",
    "ember-template-lint": "2.21.x",
    "ember-template-lint-plugin-prettier": "2.0.0",
    "ember-try": "1.4.x",
    "eslint": "7.21.x",
    "eslint-config-prettier": "8.1.x",
    "eslint-plugin-ember": "10.2.x",
    "eslint-plugin-hbs": "1.0.0",
    "eslint-plugin-node": "11.1.x",
    "eslint-plugin-prettier": "3.3.x",
    "husky": "4.3.x",
    "import-sort-style-python": "1.0.x",
    "lint-staged": "10.5.x",
    "loader.js": "4.7.x",
    "npm-run-all": "4.1.x",
    "prember": "1.0.x",
    "prember-middleware": "0.1.0",
    "prettier": "2.2.1",
    "prettier-plugin-import-sort": "0.0.6",
    "prettier-plugin-packagejson": "2.2.x",
    "qunit": "2.14.x",
    "qunit-dom": "1.6.x",
    "tailwindcss": "2.0.x",
    "tailwindcss-capsize": "1.2.x",
    "typescript": "4.2.x"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://showbie.github.io/backpack-ember"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "python",
      "options": {
        "knownFramework": [
          "@ember",
          "@glimmer"
        ],
        "knownFirstparty": [
          "dummy"
        ]
      }
    },
    ".ts": {
      "parser": "typescript",
      "style": "python",
      "options": {
        "knownFramework": [
          "@ember",
          "@glimmer"
        ],
        "knownFirstparty": []
      }
    }
  }
}
